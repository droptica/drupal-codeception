{
    "name": "droptica/drupal-codeception",
    "description": "Codeception toolset for Drupal testing.",
    "type": "package",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "fakerphp/faker": "^1.21",
        "phpunit/phpunit": "^9.6",
        "vlucas/phpdotenv": "^5.5",
        "webflo/drupal-finder": "^1.2",
        "codeception/module-db": "^3.1",
        "symfony/phpunit-bridge": "^6.2",
        "codeception/module-rest": "^3.3",
        "codeception/codeception": "^5.0",
        "phpunit/php-code-coverage": "^9.2",
        "codeception/visualception": "^1.0",
        "codeception/module-asserts": "^3.0",
        "codeception/module-webdriver": "^3.2",
        "ericmartel/codeception-email": "^1.0",
        "codeception/module-phpbrowser": "^3.0",
        "league/openapi-psr7-validator": "^0.18"
    },
    "require-dev": {
        "composer/installers": "^2.2",
        "drupal/core": "^10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Codeception\\": "src/Codeception"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
