{
    "name": "droptica/drupal-codeception",
    "description": "Codeception toolset for Drupal testing.",
    "type": "package",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "fakerphp/faker": "^1.2",
        "phpunit/phpunit": "^9.5",
        "vlucas/phpdotenv": "^5.4",
        "webflo/drupal-finder": "^1.2",
        "codeception/module-db": "^2.0",
        "codeception/module-rest": "^2.0",
        "codeception/codeception": "^4.2",
        "phpunit/php-code-coverage": "^9.2",
        "codeception/visualception": "^1.0",
        "codeception/module-asserts": "^2.0",
        "codeception/module-webdriver": "^2.0",
        "codeception/module-phpbrowser": "^1.0",
        "league/openapi-psr7-validator": "^0.18",
        "ericmartel/codeception-email-mailhog": "^1.0"
    },
    "require-dev": {
        "composer/installers": "^2.2",
        "drupal/core": "^9"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Codeception\\": "src/Codeception"
        }
    }
}
